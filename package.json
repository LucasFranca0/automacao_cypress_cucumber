    "babel-plugin-istanbul": "^7.0.0",
    "cypress": "^14.2.0",
    "dotenv": "^16.4.7",
    "esbuild": "^0.25.1",
    "istanbul-lib-coverage": "^3.2.2",
    "mochawesome": "^7.1.3",
